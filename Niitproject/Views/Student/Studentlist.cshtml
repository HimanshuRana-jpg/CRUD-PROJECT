@model List<Niitproject.Context.Student>
@{
    ViewBag.Title = "Studentlist";
}

<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Age</th>
        <th>DOB</th>
        <th>Action</th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Student_Name</td>
        <td>@item.Age</td>
        <td>@item.DOB</td>
        <td>
            <a href="@Url.Action("Studentview",new
                { item.Studentid,item.Student_Name,item.Age,item.DOB})" class="btn btn-success">Update</a>
            <a href="@Url.Action("Delete",new { item.Studentid})" class="btn btn-danger">Delete</a>
        </td>
    </tr>
    }

</table>

